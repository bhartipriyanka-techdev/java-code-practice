package com.java.program.loop;
import java.util.*;

public class Table {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter any no for table :");
		int number=sc.nextInt();
		for(int i=number;i<=20;i+=number) {
			System.out.println(number+  "*" + i +  "="  +number*i);
		}
		sc.close();
		

	}

}
